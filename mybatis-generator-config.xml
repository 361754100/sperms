<?xml version="1.0" encoding="UTF-8" ?>	
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry location="./mysql-connector-java-5.1.46.jar" />
	<context id="prod">
		<!-- RowBounds pagination -->
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- jdbc连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/sperms?useSSL=false" userId="root"
			password="123456" />

		<javaModelGenerator targetPackage="com.smart.sperms.dao.model"
			targetProject="src/main/java" />
		<sqlMapGenerator targetPackage="mapper"
			targetProject="src/main/resources" />
		<javaClientGenerator targetPackage="com.smart.sperms.dao.mapper"
			targetProject="src/main/java" type="XMLMAPPER" />

		<table tableName="sys_role" domainObjectName="SysRole">
			<property name="useActualColumnNames" value="false"/>
			<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />
		</table>

		<table tableName="caculation_method" domainObjectName="CaculationMethod">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="customer" domainObjectName="Customer">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="equipment" domainObjectName="Equipment">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="equipment_enable" domainObjectName="EquipmentEnable">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="production_stat" domainObjectName="ProductionStat">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="rent_manager" domainObjectName="RentManager">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="ROLE_ID" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="sys_menu" domainObjectName="SysMenu">
			<property name="useActualColumnNames" value="false"/>
			<generatedKey column="menu_id" sqlStatement="JDBC" identity="true" />
		</table>

		<table tableName="sys_role" domainObjectName="SysRole">
			<property name="useActualColumnNames" value="false"/>
			<generatedKey column="role_id" sqlStatement="JDBC" identity="true" />
		</table>

		<table tableName="sys_role_menu_relation" domainObjectName="SysRoleMenuRelation">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="role_id" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="users" domainObjectName="Users">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="role_id" sqlStatement="JDBC" identity="true" />-->
		</table>

		<table tableName="users_customer_relation" domainObjectName="UsersCustomerRelation">
			<property name="useActualColumnNames" value="false"/>
			<!--<generatedKey column="role_id" sqlStatement="JDBC" identity="true" />-->
		</table>

	</context>
</generatorConfiguration>